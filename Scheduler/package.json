{
  "name": "scheduler",
  "version": "1.0.0",
  "description": "Scheduling/Calendar Application",
  "main": "index.js",
  "scripts": {
    "webpack:dev": "cross-env NODE_ENV=DEV node webpack.server.js",
    "app:dev": "cross-env dotnet run",
    "build:dev": "concurrently \"npm run webpack:dev\" \"npm run app:dev\"",
    "build:prod": "cross-env npm run lintJS || exit 0 && npm run lintSCSS || exit 0 && npm run bundle",
    "bundle": "shx rm -rf ./wwwroot/client/* && webpack --config webpack.config.prod.js --mode production || exit 0",
    "checkTypes": "tsc",
    "lintJS": "cross-env eslint --color './Client/src/**/*.js'",
    "lintSCSS": "cross-env stylelint './Client/src/**/*.scss' --syntax scss",
    "test": "cross-env NODE_ENV=TEST jest --config jest.config.js",
    "testCoverage": "cross-env NODE_ENV=TEST jest --config jest.config.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FosolSolutions/schedule.web.app"
  },
  "author": "",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.1.5",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.8.0",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "3.1.1",
    "breakpoint-sass": "2.7.1",
    "compression-webpack-plugin": "2.0.0",
