    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "express": "4.16.3",
